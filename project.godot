; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Titkin"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
manager/sound/pool_3D_size=16
manager/sound/max_polyphony=32
manager/music/bus="Music"

[autoload]

PlatformGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/PlatformGlobals.gd"
ResourceGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/ResourceGlobals.gd"
ModularGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/ModularGlobals.gd"
GameGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/GameGlobals.gd"
WorldGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/WorldGlobals.gd"
TileGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/TileGlobals.gd"
PawnGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/PawnGlobals.gd"
PlayerGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/PlayerGlobals.gd"
UIGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/UIGlobals.gd"
AudioGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/AudioGlobals.gd"
SaveGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/SaveGlobals.gd"
OptimizationGlobals="*res://addons/GodotCommons-Core/Scripts/Autoloads/OptimizationGlobals.gd"
SoundManager="*res://addons/resonate/sound_manager/sound_manager.gd"
MusicManager="*res://addons/resonate/music_manager/music_manager.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
Bridge="*res://addons/playgama_bridge/bridge.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotCommons-Core/plugin.cfg", "res://addons/better-terrain/plugin.cfg", "res://addons/playgama_bridge/plugin.cfg", "res://addons/resonate/plugin.cfg")

[godot_commons-core]

ui/main_menu_level_path="res://Scenes/Levels/MainMenuLevel.tscn"
ui/pause_menu_ui_path="res://Scenes/UI/Menu/PauseMenuUI.tscn"
ui/confirm_ui_path="res://Scenes/UI/Menu/ConfirmUI.tscn"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
